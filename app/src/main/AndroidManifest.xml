<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.assignmentapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_01"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_01_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RandomColorChanger">
        <activity android:name="com.example.assignmentapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SwitchActivity"
            android:label="Hello World App" >
        </activity>
        <activity
            android:name="StudentApp"
            android:label="Student App" >
        </activity>
        <activity
            android:name=".StudentAdd"
            android:label="ADD STUDENT" >
        </activity>
        <activity
            android:name=".StudentSearch"
            android:label="SEARCH STUDENT" >
        </activity>
    </application>

</manifest>